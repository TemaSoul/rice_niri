# Vim-like navigation for MOC
menu_down           = j
menu_up             = k
go_up               = h
go                  = l

# Управление воспроизведением
pause                 = Space
stop                  = s
next                  = n
previous              = b

# Громкость (как в vim? можно сделать через , и .)
volume_down_5         = ,
volume_up_5           = .

# Работа с плейлистом
add_file              = a
add_directory         = A
delete_from_playlist  = d
clear_playlist        = C

# Поиск
search_menu           = /

# Выход
quit_client           = q
quit                  = Q
